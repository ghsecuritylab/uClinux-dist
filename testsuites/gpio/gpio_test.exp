#!/usr/bin/expect

#
#Test program to test GPIO.
#

source ../kernel_config.exp
log_file [log_file_name "$argv0"]
send_user "Starting $argv0\n"
set TITLE [title "$argv0"]

send_log "*********************************\r"
send_log "Start $TITLE\r"
send_log "*********************************\r"

set timeout 300

set case_num 0

incr case_num

spawn  /bin/sh
expect -re  "$sh_prompt"
send -s "cd $uclinux_path/linux-2.6.x/arch/ \r"
expect -re  "$sh_prompt"
while 1 { 
send -s "egrep -nr 'bfin_read_PORT|bfin_write_PORT'  * \r"
expect "\r"
expect {
         -re "bfin_write_PORT.*$sh_prompt" {
                         puts "Still exists. \n"
                         case_fail $case_num
                          }

         -re "bfin_read_PORT.*$sh_prompt" {
                         puts "Still exists. \n"
                         case_fail $case_num
                          }

         -re  "$sh_prompt" 
                      {
                        puts "\nSuccess case.\n"
                        case_pass $case_num
                        break
                        }   
         timeout 
                      {
                         puts "Fail to diff.\n"
                         case_fail $case_num
                        }   
                     
}
}

incr case_num
send -s "cd $uclinux_path/linux-2.6.x/drivers/ \r"
expect -re  "$sh_prompt"
while 1 {
send -s "egrep -nr 'bfin_read_PORT|bfin_write_PORT'  * \r"
expect "\r"
expect {
        -re "bfin_write_PORT.*$sh_prompt" {
                         puts "Still exists. \n"
                         case_fail $case_num
                          }
        -re "bfin_read_PORT.*$sh_prompt" {
                         puts "Still exists. \n"
                         case_fail $case_num
                          }

         -re  "$sh_prompt"
                      {
                        puts "\nSuccess case.\n"
                        case_pass $case_num
                        break
                        }
         timeout
                      {
                         puts "Fail to diff.\n"
                         case_fail $case_num
                        }

}
}

incr case_num
send -s "cd $uclinux_path/linux-2.6.x/sound/ \r"
expect -re  "$sh_prompt"
while 1 {
send -s "egrep -nr 'bfin_read_PORT|bfin_write_PORT'  * \r"
expect "\r"
expect {
        -re "bfin_write_PORT.*$sh_prompt" {
                         puts "Still exists. \n"
                         case_fail $case_num
                          }

         -re "bfin_read_PORT.*$sh_prompt" {
                         puts "Still exists. \n"
                         case_fail $case_num
                          }

         -re  "$sh_prompt"
                      {
                        puts "\nSuccess case.\n"
                        case_pass $case_num
                        break
                        }
         timeout
                      {
                         puts "Fail to diff.\n"
                         case_fail $case_num
                        }

}
}


send_log "\n"
send_log "\n$TITLE ............\[PASS\]\n"

send_user "Ending $argv0\n"
log_file

