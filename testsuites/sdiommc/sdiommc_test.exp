        }
    }
}
send "ls /\r"
while 1 {
    expect {
        -re "runcp\\\.sh.*$kernel_prompt" {
            send_user "successfully mount MMC/SD as the root filesystem\r"
            case_pass $case_num
            break
        }
        timeout {
            case_fail $case_num
        }
    }
}

send_log "\n"
