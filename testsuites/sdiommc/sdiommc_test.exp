step "Start bonnie++ test"

set timeout 10
send "fdisk /dev/$mmc_dev\r"
expect -re "Command.*:"
send "p\r"
set timeout 10
while 1 {
    expect {
        -re "/dev/$mmc_dev1" {
            send "d\r"
            expect -re "Partition number.*"
            send "1\r"
            expect -re "Command.*"
            break
        }
        eof {
            break
        }
        timeout {
            break
        }
    }
}

sleep 2
send "p\r"
while 1 {
    expect {
        -re "/dev/$mmc_dev2" {
            send "d\r"
            expect -re "Partition number.*"
            send "2\r"
            expect -re "Command.*"
            send "w\r"
            expect -re $kernel_prompt
            break
        }
        eof {
            send "q\r"
            break
        }
        timeout {
            send "q\r"
            break
        }
    }
}


set timeout 10
send "fdisk /dev/$mmc_dev\r"
expect -re "Command.*:"
send "n\r"
expect -re "primary partition.*"
send "p\r"
expect -re "Partition number.*"
send "1\r"
expect -re "First cylinder.*"
send "\r"
expect -re "Last cylinder.*"
send "\r"
expect -re "Command.*"
send "w\r"
expect -re $kernel_prompt

